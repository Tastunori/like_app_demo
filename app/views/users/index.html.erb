

<h1 class="index_top">「ありがとう」を送ろう</h1>


  <% @users.each do |user| %>
     <% if @current_user != user %>
    <div class="user_index">
     <div class="user_left">
       <img src = "<%= "/user_images/#{user.image_name}" %>" >
     </div>
     <div class="user_right">
       <%= link_to(user.name, "/posts/#{user.id}")  %>
     </div>
   </div>
   <% end %>
  <% end %>
